    "@radix-ui/react-tooltip": "^1.2.8",
    "@stdlib/stats-base-dists-beta-pdf": "^0.2.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.86.0",
    "@t3-oss/env-nextjs": "^0.13.8",
