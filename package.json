  "engines": {
    "node": ">=22.0.0"
  },
  "browserslist": {
    "production": [
      "chrome >= 107",
      "edge >= 107",
      "firefox >= 106",
      "safari >= 16",
      "ios >= 16"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "supabase",
      "unrs-resolver"
    ]
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}
